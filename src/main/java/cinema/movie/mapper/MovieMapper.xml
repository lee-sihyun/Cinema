<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cinema.movie.mapper.MovieMapper">



 
	<select id="selectMv" resultType="MovieDTO">
		select DISTINCT mv_name, mv_cast,mv_director,mv_num from movie order by mv_num desc
	</select> 



	<select id="reSelectMv" resultType="MovieDTO">
		select DISTINCT mv_name from movie
	</select> 








</mapper>